{
  "version": 6,
  "cmakeMinimumRequired": {
      "major": 3,
      "minor": 26,
      "patch": 1
  },
  "configurePresets": [
    {
      "name": "debug-settings",
      "hidden": true,
      "debug": {
        "find": false,
        "output": false,
        "tryCompile": false
      },
      "errors": {
        "deprecated": true,
        "dev": true
      },
      "warnings": {
        "deprecated": true,
        "dev": true,
        "systemVars": true,
        "uninitialized": false,
        "unusedCli": true
      }
    }
  ]
}
